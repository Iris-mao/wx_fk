/**app.wxss**/
page{
  color: #333;
  font-family: "微软雅黑";
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.wrap95{
  width: 95%;
  margin:0 auto;
}
.clear{
  clear:both;
}
.dis{
  width: 100%;
  height: 30rpx;
  background: #eee;
}

/* head */
image{width: 100%;}

.head image{
  height: 450rpx;
}

/* foot  */
.foot .top{
  padding-top: 20rpx;
}
.foot .top .top-left{
  width: 35%;
  float: left;
  text-align: center;
   padding-bottom: 5%; 
}
.foot .top .top-left image{
  width: 250rpx;
  height: 250rpx;
}
.foot .top .top-left view{
  color: #999;
  font-size: 34rpx;
}
.foot .top .top-right{
  width: 63%;
  float: right;
   padding-bottom: 5%; 
}
.foot .top .top-right .wrap{
  border-left: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
  height: 130rpx;
  padding-left: 5%;
  padding-top: 5%;
  background: url(http://4g.sztjfk.cn/resources/foot-tel.png) right center no-repeat;
  background-size: 20%;
}
.foot .top .top-right .wrap:last-child{
  border-bottom: none;
  background: url(http://4g.sztjfk.cn/resources/foot-qq.png) right center no-repeat;
  background-size: 20%;
}
.foot .top .top-right .wrap view:first-child{
  font-size: 30rpx;
  color: #999;
}
.foot .top .top-right .wrap view:last-child{
  font-size: 45rpx;
  font-weight: bold;
}
.foot .down{
  height: 150rpx;
  background: #fe6397 url(http://4g.sztjfk.cn/resources/foot-back.png) center no-repeat;
  background-size: 100%;
  text-align: center;
  position: fixed;
  bottom: 0;
  width: 100%;
}
.foot .down > view{
  width:25%;
  float:left;
  color: #fff;
  font-size: 30rpx;
  height: 150rpx;
  line-height: 230rpx;
}
.foot .down > view:first-child{
  background: url(http://4g.sztjfk.cn/resources/foot-1.png) center 35% no-repeat;
  background-size: 40%;
}
 .foot .down > view:nth-child(2){
  background: url(http://4g.sztjfk.cn/resources/foot-2.png) center 35% no-repeat;
  background-size: 40%;
  
} 
.foot .down > view:nth-child(3){
  background: url(http://4g.sztjfk.cn/resources/foot-3.png) center 35% no-repeat;
  background-size: 30%;
}
.foot .down > view:last-child{
  background: url(http://4g.sztjfk.cn/resources/foot-4.png) center 35% no-repeat;
  background-size: 30%;
}

/*模板 docone  */
.doc-one{
  margin-top: 20rpx;
  margin-bottom: 50rpx;
  padding: 20rpx 0;
  background: #f1f1f1;
}
.doc-one .one-l{
  width: 25%;
  float: left;
  text-align: center;
}
.doc-one .one-l image{
  width: 160rpx;
  height: 130rpx;
}
.doc-one .one-m{
  width: 50%;
  float: left;
  margin: 0 2%;
}
.doc-one .one-m .doc-name{
  font-size: 40rpx;
  font-weight: bold;
}
.doc-one .one-m .doc-pos{
  font-size: 30rpx;
  color: #666;
}
.doc-one .one-m .doc-zhishu{
  font-size: 30rpx;
  color: #fe6397;
}
.doc-one .one-m .doc-zhishu image{
  width: 120rpx;
  height: 25rpx;
}
.doc-one .one-r{
  width: 20%;
  float: left;
  text-align: center;
}
.doc-one .one-r image{
  width: 120rpx;
  height: 120rpx;
}
.doc-one .doc-sc{
  padding: 0 2%;
  font-size: 30rpx;
}


/*模板 pinglun  */
.main-title{
  font-size: 40rpx;
  color: #333;
  font-family: "微软雅黑";
  padding-bottom: 20rpx;
}
.comment{
  border-top:2rpx solid #e5e5e5;
  padding-top: 20rpx;
}
.pingjia .pingjia-left{
  width: 30%;
  float: left;
  height: 250rpx;
  background: #fe6397 url(http://4g.sztjfk.cn/resources/main4-1.png) center 80% no-repeat;
  background-size: 90%;
  border-radius: 5%;
  text-align: center;
  color: #fff;
  font-family: "微软雅黑";
  font-size: 40rpx;
  padding-top: 40rpx;
}
.pingjia .pingjia-left .point{
  font-family: "Impact";
  font-size: 56rpx;
  padding-top: 10rpx;
}
.pingjia .pingjia-right{
  width: 68%;
  float: right;
}
.pingjia .pingjia-right > text{
    font-family: "微软雅黑";
    color: #333;
    font-size: 26rpx;
    height: 80rpx;
    line-height: 80rpx;
    border: 1px solid #ccc;
    width: 45%;
    margin-left: 2%;
    float: left;
    text-align: center;
    border-radius: 5px;
    margin-bottom: 20rpx;
    padding-right: 2%;
}
.pingjia .pingjia-right > text text{
    float: right;
    color: #ec7729;
    font-weight: bold;
}
.pinglun{
  border-top: 1px solid #ccc;
  padding: 20rpx 0;
}
.pinglun .left{
  width: 20%;
  float:left;
}
.pinglun .left image{
  width: 120rpx;
  height: 120rpx;
}
.pinglun .right{
  width: 78%;
  float:right;
  padding-bottom: 2%;
}
.pinglun .right .name{
  color: #f32067;
  font-family: "微软雅黑";
  font-size: 40rpx;
  background: url(http://4g.sztjfk.cn/resources/main4-back.png) right center no-repeat;
  background-size: 30%;
  padding-bottom:6rpx;
}
.pinglun .right .zhengwen text,
.pinglun .right .zhengwen > view{
  font-family: "微软雅黑";
  color: #333;
  font-size: 26rpx;
  line-height: 40rpx;
  display: inline-block;
}
.pinglun .right .zhengwen image{
  width: 220rpx;
  height: 150rpx;
  margin: 2% 2% 2% 0;
}

/*病种  */
.bingzhong .bz-top{
  border-bottom: 1px solid #e5e5e5;
}
.bingzhong .bz .bz-wrap{
  width: 33%;
  text-align: center;
  float: left;
  font-size: 32rpx;
  border-right: 1px solid #e5e5e5;
  padding: 5% 0;
}
.bingzhong .bz .bz-wrap image{
  width: 120rpx;
  height: 120rpx;
}
