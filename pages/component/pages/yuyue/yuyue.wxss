/* pages/component/pages/yuyue/yuyue.wxss */
.yuyue{
  padding-bottom: 150rpx;
  margin-top:-10rpx;
}
.yuyue .yuyue-title{
  background: #f3f1f4;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 3%;
  font-size: 36rpx;
}
.yuyue .yuyue-form view{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #f3f1f4;
  font-size: 34rpx;
}
.yuyue .yuyue-form .yuyue-label{
  float: left;
  width: 30%;
}
.yuyue .yuyue-form .yuyue-input{
  width: 70%;
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  display: inline-block;
}
.psection{
    color: #666;
    font-size: 34rpx;
    font-family: "微软雅黑";
    /* text-indent: 10rpx; */
}
.yuyue .yuyue-form .yuyue-btn{
  height: 300rpx;
  background: #edeced;
  text-align: center;
}
.yuyue .yuyue-btn button{
  width: 300rpx;
  height: 80rpx;
  display: inline-block;
  margin: 5% 2% 0;
  line-height: 80rpx;
  color: #fff;
  background: #ed9017 url(http://4g.sztjfk.cn/resources/yuyue-1.png) 12% center no-repeat;
  background-size: 18%;
  border-radius: 0;
  border: none;
  padding-left: 10%;
}
.yuyue .yuyue-btn button.reset{
  background: #fe6397 url(http://4g.sztjfk.cn/resources/yuyue-2.png) 12% center no-repeat;
  background-size: 18%;
}
.yuyue .yuyue-btn view{
  width: 60%;
  height: 100rpx;
  line-height: 100rpx;
  color: #fe6397;
  border:1px solid #fe6397;
  border-radius:100rpx;
  margin: 0 auto;
}

.foot .top .top-left{
  padding-bottom: 25%;
}

.foot .top .top-right{
  padding-bottom: 25%;
}